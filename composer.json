{
	"name": "sowapps/so-core",
	"type": "symfony-bundle",
	"license": "MIT",
	"keywords": ["symfony-bundle"],
	"require": {
		"php": ">=8.1",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"ext-intl": "*",
		"symfony/framework-bundle": "^7.0",
		"symfony/security-bundle": "^7.2",
		"symfony/twig-bundle": "6.*",
		"twig/cssinliner-extra": "3.*",
		"twig/extra-bundle": "^2.12|^3.0",
		"twig/twig": "^3.0"
	},
	"require-dev": {
		"symfony/phpunit-bridge": "^7.3",
		"phpunit/phpunit": "^12.4"
	},
	"autoload": {
		"psr-4": {
			"Sowapps\\SoCore\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Sowapps\\SoCore\\Tests\\": "tests/"
		}
	},
	"suggest": {
		"doctrine/orm": "Requis si vous activez les fonctionnalit√©s Doctrine de ce bundle"
	},
	"authors": [
		{
			"name": "Florent HAZARD",
			"email": "contact@sowapps.com",
			"homepage": "https://sowapps.com",
			"role": "Developer"
		}
	]
}
